<h1>LogIn</h1>
<%= link_to "Back to Trainlines", trainlines_path %>
<hr>
<%= form_tag sessions_path do %>
  <div>
    <%= label_tag "email" %><br/>
    <%= text_field_tag "email" %>
  </div>
  <div>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div><%= submit_tag "Log In" %></div>
<% end %>
